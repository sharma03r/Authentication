<% if(user){%>
<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Dashboard</h1>
    <p class="lead mb-0">Welcome <%= user.name %></p>
    <p class="mt-0">Your Email ID: <%= user.email %></p>
    <a href="/users/sign-out" class="btn btn-primary">Sign Out</a>
  </div>
</div>
<%}else{%>
<p>You Must Be Logged-In to View the Details.</p>
<%}%>
